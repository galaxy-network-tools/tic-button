{
  "manifest_version": 2,
  "name": "NBNP TIC-Button",
  "version": "2.1",
  "author": "Boobie,worp1900",
  "description": "Sendet Scans und Taktik von Galaxy Network ans TIC. Adaptiert von Boobies New TIC-Button. Icons made by surang: https://www.flaticon.com/authors/surang",
  "homepage_url": "https://www.navikatzen.com/tic2",

  "icons": {
    "96": "icon.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "ticaddon@worp.io"
    }
  },

  "content_scripts": [{
    "matches": ["https://galaxy-network.de/*"],
    "js": ["default.js"]
  }],

  "permissions": [
    "*://www.galaxy-network.net/*",
    "https://www.navikatzen.com/tic2/*",
    "activeTab",
    "contextMenus",
    "webRequest",
    "webRequestBlocking"
  ]
}
